{
  "plugins": ["prettier"],
  "extends": [
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:react-hooks/recommended",
    "prettier",
    "eslint:recommended",
    "next/core-web-vitals"
  ],
  "rules": {
    "react/react-in-jsx-scope": 0,
    "react/jsx-uses-react": 0,
    "react/prop-types": 0
  },
  "globals": {
    "JSX": "readonly",
    "document": false
  },
  "env": {
    "browser": true,
    "node": true
  }
}
